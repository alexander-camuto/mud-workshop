{
  "name": "mud-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "forge --version || (pnpm foundryup && forge --version)",
    "foundryup": "curl -L https://foundry.paradigm.xyz | bash && bash ~/.foundry/bin/foundryup && pnpm run vercel:link-foundry",
    "vercel:link-foundry": "([ \"$VERCEL\" ] && ln -sf /vercel/.foundry/bin/* node_modules/.bin/) || true",
    "build": "mud build && mud abi-ts --extension .d.json.ts && vite build",
    "build:local": "env $(grep -v '#' .env | xargs) pnpm run build",
    "build:testnet": "env $(grep -v '#' .env.testnet | xargs) pnpm run build",
    "dev": "pnpm run dev:local",
    "dev:local": "env $(grep -v '#' .env | xargs) mprocs --config mprocs.local.yaml",
    "dev:testnet": "env $(grep -v '#' .env.testnet | xargs) mprocs --config mprocs.testnet.yaml",
    "deploy:testnet": "env $(grep -v '#' .env.testnet | xargs) mprocs --config mprocs.testnet.deploy.yaml",
    "lint": "eslint .",
    "test": "tsc",
    "mud:up": "mud set-version --tag main && pnpm install",
    "automine": "pnpm tsx src/scripts/enableAutomine.ts",
    "zerofee": "pnpm tsx src/scripts/setZeroFee.ts",
    "chainconfig": "pnpm automine && pnpm zerofee"
  },
  "dependencies": {
    "@ark/util": "^0.18.0",
    "@latticexyz/common": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/config": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/explorer": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/protocol-parser": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/schema-type": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/stash": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/store": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/store-sync": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/world": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/world-module-crosschain": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@latticexyz/world-modules": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "optimism": "https://github.com/ethereum-optimism/optimism#ecdb788a18022d4b78d6288c1f6ec94c152d02f8",
    "@tanstack/react-query": "^5.56.2",
    "abitype": "^1.0.7",
    "fast-deep-equal": "^3.1.3",
    "nodemon": "^3.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.5.2",
    "viem": "2.21.19",
    "wagmi": "2.12.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@latticexyz/cli": "2.2.15-crosschain-08179949754e0b748e52ad2a51e7a4aa2547795e",
    "@types/node": "^22.10.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "globals": "^15.9.0",
    "mprocs": "^0.7.1",
    "postcss": "^8.4.47",
    "supersim": "^0.0.5",
    "tailwindcss": "^3.4.11",
    "tsx": "^4.19.2",
    "typescript": "5.4.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
